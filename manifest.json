{
  "manifest_version": 2,
  "name": "GitHub Diff Expander",
  "description": "Expand diffs in GitHub & GHE with one click!",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "GitHub Diff Expander"
  },
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "permissions" : [
    "declarativeContent",
    "activeTab"
  ]
}
